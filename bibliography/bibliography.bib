@article{ciri_model-free_2017-1,
  title = {Model-Free Control of Wind Farms: {{A}} Comparative Study between Individual and Coordinated Extremum Seeking},
  shorttitle = {Model-Free Control of Wind Farms},
  author = {Ciri, Umberto and Rotea, Mario A. and Leonardi, Stefano},
  year = {2017},
  month = dec,
  volume = {113},
  pages = {1033--1045},
  issn = {0960-1481},
